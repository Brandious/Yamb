{
  "name": "@brandious/yamb",
  "version": "1.0.0",
  "description": "YAMB game based on TS and Websockets",
  "main": "main.js",
  "workspaces": {
    "packages": [
      "workspaces/*"
    ]
  },
  "scripts": {
    "build:server": "yarn workspace @yamb/server build",
    "start:dev:client": "yarn workspace @yamb/client start:dev",
    "start:dev:server": "yarn workspace @yamb/server start:dev",
    "start:prod:client": "yarn workspace @yamb/client start:prod",
    "start:prod:server": "yarn workspace @yamb/server build && yarn workspace @yamb/server start:prod"
  },
  "private": true,
  "repository": "https://github.com/Brandious/Yamb.git",
